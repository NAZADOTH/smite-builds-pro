{
  "exclude": ["node_modules", "dist"],
  "compilerOptions": {
    "target": "es2017", // Otherwise complains about string.padStart.
    "moduleResolution": "node", // Otherwise complains about libraries.
    "outDir": "dist", // Otherwise complains: Cannot write file 'vite.config.js' because it would overwrite input file.
    "allowJs": true,
    "strict": true
  }
}
